            <script>// msg to FHIR Bundle zawierający ServiceRequest

try {
    // Pobieramy ServiceRequest
    var sr = msg[&apos;entry&apos;][0][&apos;resource&apos;];

    var serviceRequestId = sr[&apos;id&apos;] || null;
    var status = sr[&apos;status&apos;] || null;
    var priorytet = sr[&apos;priority&apos;] || null;
    var intent = sr[&apos;intent&apos;] || null;
    var category = (sr[&apos;category&apos;] &amp;&amp; sr[&apos;category&apos;][0] &amp;&amp; sr[&apos;category&apos;][0][&apos;coding&apos;] &amp;&amp; sr[&apos;category&apos;][0][&apos;coding&apos;][0][&apos;display&apos;]) || null;
    var code = (sr[&apos;code&apos;] &amp;&amp; sr[&apos;code&apos;][&apos;coding&apos;] &amp;&amp; sr[&apos;code&apos;][&apos;coding&apos;][0][&apos;display&apos;]) || null;
    var authoredOn = sr[&apos;authoredOn&apos;] || null;
    var reasonCode = (sr[&apos;reasonCode&apos;] &amp;&amp; sr[&apos;reasonCode&apos;][0] &amp;&amp; sr[&apos;reasonCode&apos;][0][&apos;text&apos;]) || null;
    var note = (sr[&apos;note&apos;] &amp;&amp; sr[&apos;note&apos;][0] &amp;&amp; sr[&apos;note&apos;][0][&apos;text&apos;]) || null;
    var patientRef = sr[&apos;subject&apos;] ? sr[&apos;subject&apos;][&apos;reference&apos;] : null;
    
    channelMap.put(&apos;pat&apos;, patientRef);  // zapis do channelMap
    channelMap.put(&apos;serviceRequestId&apos;, serviceRequestId);
    channelMap.put(&apos;status&apos;, status);
    channelMap.put(&apos;intent&apos;, intent);
    channelMap.put(&apos;category&apos;, category);
    channelMap.put(&apos;code&apos;, code);
    channelMap.put(&apos;authoredOn&apos;, authoredOn);
    channelMap.put(&apos;reasonCode&apos;, reasonCode);
    channelMap.put(&apos;note&apos;, note);
    channelMap.put(&apos;priorytet&apos;, priorytet);
    // Logowanie do debugu
    logger.info(&quot;ServiceRequest ID: &quot; + serviceRequestId);
    logger.info(&quot;Code: &quot; + code);
    logger.info(&quot;Note: &quot; + note);
    var sas = {
        resourceType: &quot;ServiceRequest&quot;,
        id: serviceRequestId,
        status: &quot;draft&quot;
    };

    // Zamiana na JSON string
    var sss = JSON.stringify(sas);

    // Zapis do channelMap
    channelMap.put(&apos;sss&apos;, sss);
} catch (e) {
    logger.error(&quot;Błąd w JavaScript Writer: &quot; + e);
}</script>
