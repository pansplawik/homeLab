            <script>// msg jest JSON-em Pacjenta
try {
    // --- ID pacjenta ---
    var patientId = msg[&apos;id&apos;] || null;
    channelMap.put(&apos;patientId&apos;, patientId);

    // --- MRN (identifier) ---
    var mrn = (msg[&apos;identifier&apos;] &amp;&amp; msg[&apos;identifier&apos;][0] &amp;&amp; msg[&apos;identifier&apos;][0][&apos;value&apos;]) || null;
    channelMap.put(&apos;mrn&apos;, mrn);

    // --- Nazwisko ---
    var lastName = (msg[&apos;name&apos;] &amp;&amp; msg[&apos;name&apos;][0] &amp;&amp; msg[&apos;name&apos;][0][&apos;family&apos;]) || null;
    channelMap.put(&apos;lastName&apos;, lastName);

    // --- Imię ---
    var firstName = (msg[&apos;name&apos;] &amp;&amp; msg[&apos;name&apos;][0] &amp;&amp; msg[&apos;name&apos;][0][&apos;given&apos;] &amp;&amp; msg[&apos;name&apos;][0][&apos;given&apos;][0]) || null;
    channelMap.put(&apos;firstName&apos;, firstName);

    // --- Prefix (np. Pan/Pani) ---
    var prefix = (msg[&apos;name&apos;] &amp;&amp; msg[&apos;name&apos;][0] &amp;&amp; msg[&apos;name&apos;][0][&apos;prefix&apos;] &amp;&amp; msg[&apos;name&apos;][0][&apos;prefix&apos;][0]) || null;
    channelMap.put(&apos;prefix&apos;, prefix);

    // --- Telefon ---
    var phone = (msg[&apos;telecom&apos;] &amp;&amp; msg[&apos;telecom&apos;][0] &amp;&amp; msg[&apos;telecom&apos;][0][&apos;value&apos;]) || null;
    channelMap.put(&apos;phone&apos;, phone);

    // --- Płeć ---
    var gender = msg[&apos;gender&apos;] || null;
    channelMap.put(&apos;gender&apos;, gender);

    // --- Data urodzenia ---
    var birthDate = msg[&apos;birthDate&apos;] || null;
    channelMap.put(&apos;birthDate&apos;, birthDate);

